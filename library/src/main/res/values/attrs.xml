<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BingView">
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeColor" format="color" />
        <attr name="innerCircleRadius" format="float" />
    </declare-styleable>
    <declare-styleable name="LEDView">
        <attr name="ledColor" format="color" />
        <attr name="ledNumber" format="string" />
        <attr name="ledPointWidth" format="dimension" />
        <attr name="ledIntervalWidth" format="dimension" />
        <attr name="ledAutoScroll" format="dimension" />
        <attr name="ledSingleLine" format="dimension" />
    </declare-styleable>
    <declare-styleable name="InstallmentView">
        <attr name="partCount" format="integer" />
        <attr name="partWidth" format="dimension" />
        <attr name="selectedPartWidth" format="dimension" />
        <attr name="lineHeight" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="color" format="color" />
        <attr name="selectedColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TicketDividerView">
        <attr name="td_endColor" format="color" />
        <attr name="td_lineColor" format="color" />
        <attr name="td_lineHeight" format="dimension" />
        <attr name="td_strokeWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MyTextView">
        <attr name="tv_lineNumber" format="integer" />
        <attr name="tv_textColor" format="color" />
        <attr name="tv_textSize" format="dimension" />
        <attr name="tv_text" format="string" />
    </declare-styleable>
</resources>